% 微分記号(ローマン体)
\newcommand{\diffd}{\mathrm{d}} 
% 微分
\newcommand{\diff}[2]{\frac{\mathrm{d}{#1}}{\mathrm{d}{#2}}}
\newcommand{\difn}[3]{\frac{\mathrm{d}^{#1}{#2}}{\mathrm{d}{#3}^{#1}}}
% 偏微分
\newcommand{\pdiff}[2]{\frac{\partial{#1}}{\partial{#2}}}
\newcommand{\pdifn}[3]{\frac{\partial^{#1}{#2}}{\partial{#3}^{#1}}}
% 積分
\newcommand{\integralone}[4]{\int_{#1}^{#2} {#3} \mathrm{d}{#4}}
\newcommand{\ointegral}[3]{\oint_{#1} {#2} \mathrm{d}{#3}}
\newcommand{\integraltwo}[7]{\int_{#1}^{#2} \int_{#3}^{#4} {#5} \mathrm{d}{#6} \mathrm{d}{#7}}
